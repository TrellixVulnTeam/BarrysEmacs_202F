; 
; electric-c key bindings
; 
    (local-bind-to-key "JavaScript-paren" ")")
    (local-bind-to-key "JavaScript-paren" "]")
    (local-bind-to-key "JavaScript-paren" "}")
    (local-bind-to-key "JavaScript-fore-paren" "\e)")
    (local-bind-to-key "JavaScript-back-paren" "\e(")
    (local-bind-to-key "JavaScript-high-voltage-on" "\^^B")

    ;
    ;	vt200 bindings
    ; 
    (local-bind-to-key "JavaScript-back-paren" "\(f6)")
    (local-bind-to-key "JavaScript-high-voltage-on" "\(f8)")
    (local-bind-to-key "JavaScript-fore-paren" "\(f10)")

(defun
    (high-voltage-on-key-binding
	(local-bind-to-key "JavaScript-high-voltage-on" "\^^")
	(local-bind-to-key "JavaScript-high-voltage-on" "\(f8)")
	(local-bind-to-key "tab-key" "\^i")
    )
)
(defun
    (high-voltage-off-key-binding
	(local-bind-to-key "JavaScript-high-voltage-off" "\^^")
	(local-bind-to-key "JavaScript-high-voltage-off" "\(f8)")
	(local-bind-to-key "tab-key" "\^i")
    )
)
