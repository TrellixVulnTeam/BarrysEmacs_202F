(bind-to-key "gtags-goto-definition" "\^xD")
(bind-to-key "gtags-goto-reference" "\^xR")
(bind-to-key "gtags-goto-symbol" "\^xS")
(bind-to-key "gtags-goto-definition-in-buffer" "\^xd")
(bind-to-key "gtags-goto-reference-in-buffer" "\^xr")
(bind-to-key "gtags-goto-symbol-in-buffer" "\^xs")

(bind-to-key "gtags-goto-definition" "\[menu]GD")
(bind-to-key "gtags-goto-reference" "\[menu]GR")
(bind-to-key "gtags-goto-symbol" "\[menu]GS")
(bind-to-key "gtags-goto-definition-in-buffer" "\[menu]Gd")
(bind-to-key "gtags-goto-reference-in-buffer" "\[menu]Gr")
(bind-to-key "gtags-goto-symbol-in-buffer" "\[menu]Gs")

(UI-add-menu-keys "\[menu]GD" 65 "&GTags" 10 "Goto definition...")
(UI-add-menu-keys "\[menu]GR" 65 "&GTags" 20 "Goto reference...")
(UI-add-menu-keys "\[menu]GS" 65 "&GTags" 30 "Goto symbol...")
(UI-add-menu-keys "-"	 65 "&GTags" 40 "Sep1")
(UI-add-menu-keys "\[menu]Gd" 65 "&GTags" 50 "Goto &definition in buffer")
(UI-add-menu-keys "\[menu]Gr" 65 "&GTags" 60 "Goto &reference in buffer")
(UI-add-menu-keys "\[menu]Gs" 65 "&GTags" 70 "Goto &symbol in buffer")
