; 
; electric-c key bindings
; 
    (local-bind-to-key "Java-paren" ")")
    (local-bind-to-key "Java-paren" "]")
    (local-bind-to-key "Java-paren" "}")
    (local-bind-to-key "Java-fore-paren" "\e)")
    (local-bind-to-key "Java-back-paren" "\e(")
    (local-bind-to-key "Java-high-voltage-on" "\^^B")

(defun
    (high-voltage-on-key-binding
	(local-bind-to-key "Java-high-voltage-on" "\^^")
	(local-bind-to-key "tab-key" "\^i")
    )
)
(defun
    (high-voltage-off-key-binding
	(local-bind-to-key "Java-high-voltage-off" "\^^")
	(local-bind-to-key "tab-key" "\^i")
    )
)
