[barrys-emacs-testing]
name=Barrys Emacs for Fedora $releasever - $basearch - Testing
failovermethod=priority
baseurl=http://barrys-emacs.org/repos/fedora-$releasever/barrys-emacs-testing/$basearch/release/
enabled=1
metadata_expire=7d
repo_gpgcheck=0
type=rpm
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-barrys-emacs-$releasever-$basearch
skip_if_unavailable=False

